import java.util.ArrayList;
import java.util.Iterator;

Import java.util.Scanner;
public class calcula1 {

   public static void main (string[] args) {
         Scanner leer = new Scanner(System.in);
         int n,a,b;
         System.out.println(x: "Ingresa 2 numero");
         a = leer.nextInt();
         b = leer.nextInt();
         System.out.println(x: "Ingresa una opcion: \n 1= suma\2= resta\n3= multiplicacion\n4= division");
         n = leer.nextInt();
         switch(n){
             case 1:
                 System.out.println("La suma de los 2 numeros es: "+(a+b));
                 break;
             case 2:
                 System.out.println("La suma de los numeros es: "+(a-b));
                 break;
             case 3:
                 System.out.println("La multiplicacion de los 2 numeros es: "+(a*b));
                 break;
             case 1:
                 System.out.println("La multiplicacion de los 2 numeros es: "+(a/b));
                 break;
             default:
                 System.out.println(x: "Ingresa un numero entre 1 y 4");                     

         } 

   }

} 
